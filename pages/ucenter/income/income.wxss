
.income-item{
	height: 132rpx;
	display: flex;
	align-items: center;
}
.income_price{
	color: #19a918;
	font-size: 32rpx;
}
.income_tag{
	width: 32rpx;
	height: 32rpx;
	display: flex;
	align-items: center;
	justify-content: center;
	font-size: 20rpx;
	color: #fff;
	background: #ea3341;
	border-radius: 2rpx;
}
.income_tag--jj{
	background: #f86e21;
}
.income_tag--kk{
	background: #fff;
}