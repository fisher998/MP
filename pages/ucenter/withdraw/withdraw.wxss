.withdraw{
	display: flex;
	flex-direction: column;
	justify-content: space-between;
	width: 686rpx;
	height: 403rpx;
	background: #fff;
	margin: 32rpx 32rpx 0;
	border-radius: 4rpx;
	overflow: hidden;
	padding: 48rpx 48rpx 0rpx;
	/* border:1px solid #d8d8d8; */
}
.withdraw_label{
	font-size: 26rpx;
	line-height: 94rpx;
}
.withdraw_price{
	display: flex;
	align-items: center;
	font-size: 38rpx;
	line-height: 1;
}
.withdraw_account{
	display: flex;
	align-items: center;
	font-size: 32rpx;
	line-height: 1;
}
button {
    font-size: 16px;
		border-radius: 4rpx;
}
