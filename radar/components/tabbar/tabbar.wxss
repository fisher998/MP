.tabbar-box {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  background: white;
  /* box-shadow: 0 -4rpx 9rpx rgba(0, 0, 0, 0.05); */
  display: flex;
  justify-content: space-between;
  align-items: center;
  z-index: 999;
  font-size: 22rpx;
  border-top: 1px solid #f3f3f3;
  padding: 5px 0;
}
.ipx{
	/* padding: 20rpx 0; */
	padding-bottom:68rpx;
}
.tabbar-box-place {
  height: 60px;
}

.tabbar-box-item {
  position: relative;
  flex: 1;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.tabbar-box-item__img {
  width: 28px;
  height: 28px;
  margin: 0 0;
}

.tabbar-box-item__text {
  text-align: center;
  color: #838591;
  font-size: 10px;
  line-height: 1.5;
}


